package reflect;

public class ShaaTest {
	public static void main(String[] args) {
		
		
		int b = 0;
		int c = 1;
		
		c +=  2;
		
		b -= 2;
		
		
		System.out.println(c);
		
	}
	
	
	// a++:先使用a，然后做加1操作
	// ++a：先做+1操作，然后使用a
	
	// 一个排序数组中给定数出现的次数，给出测试用例
	public int getTimes(int[] a, int n) {
		int times = 0;
		for (int i = 0; i < a.length; i++) {
			if (n == a[i]) {
				times++;
			}
		}
		return times;
	}
}
